        "@nrwl/nx-cloud": "13.1.2",
        "@nrwl/react": "^12.0.0",
        "@nrwl/tao": "13.4.6",
        "@nrwl/workspace": "13.4.6",
        "@types/jest": "27.0.2",
        "@types/node": "14.14.33",
        "@typescript-eslint/eslint-plugin": "~5.3.0",
        "@typescript-eslint/parser": "~5.3.0",
