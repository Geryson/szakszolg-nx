{
    "name": "szakszolg-nx",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "rmdir": "node -e \"var fs = require('fs'); process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true })); process.exit(0);\"",
        "docs:clean-api": "del-cli --force ./docs/api",
        "docs:clean-szakszolg": "del-cli --force ./docs/szakszolg",
        "docs:clean": "yarn run docs:clean-api && yarn run docs:clean-szakszolg",
        "docs:generate": "yarn run docs:clean && npx compodoc -p tsconfig.api-doc.json -d docs/api/ && npx compodoc -p tsconfig.szakszolg-doc.json -d docs/szakszolg/",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && node tools/scripts/download-cert.js",
        "res": "npx ts-node tools/scripts/resource-generator.ts",
        "start": "node dist/apps/api/main.js",
        "build": "nx build api --optimization=false",
        "test": "nx test"
    },
    "engines": {
        "node": "16.x",
        "yarn": ">= 1.19.1"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.2.0",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "@capacitor/core": "^3.2.5",
        "@compodoc/compodoc": "^1.1.18",
        "@ionic/angular": "^5.8.3",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.1.6",
        "@nestjs/core": "^8.0.0",
        "@nestjs/graphql": "^9.1.2",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mongoose": "^9.0.2",
        "@nestjs/passport": "^8.1.0",
        "@nestjs/platform-express": "^8.0.0",
        "@nrwl/angular": "13.4.6",
        "@nxtend/capacitor": "12.1.0",
        "@nxtend/ionic-angular": "12.1.0",
        "add": "2.0.6",
        "apollo-server-express": "^3.6.1",
        "bcryptjs": "^2.4.3",
        "class-validator": "^0.13.2",
        "del-cli": "^4.0.1",
        "dotenv": "^14.2.0",
